{
  "name": "@gacabartosz/gaca-core",
  "version": "2.0.0",
  "description": "GACA-Core - Universal AI Bus with dynamic providers, models, ranking, and failover. Framework-agnostic with dependency injection.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.js"
    },
    "./interfaces": {
      "types": "./dist/core/interfaces/index.d.ts",
      "import": "./dist/core/interfaces/index.js",
      "require": "./dist/core/interfaces/index.js"
    },
    "./loggers": {
      "types": "./dist/core/loggers/index.d.ts",
      "import": "./dist/core/loggers/index.js",
      "require": "./dist/core/loggers/index.js"
    },
    "./persistence": {
      "types": "./dist/core/persistence/index.d.ts",
      "import": "./dist/core/persistence/index.js",
      "require": "./dist/core/persistence/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gacabartosz/gaca-core.git"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "tsx watch src/api/server.ts",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "build:server": "tsc",
    "start": "node dist/api/server.js",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "seed": "tsx scripts/seed.ts",
    "seed:force": "tsx scripts/seed.ts --force",
    "sync": "tsx scripts/sync-providers.ts",
    "sync:dry": "tsx scripts/sync-providers.ts --dry-run",
    "auto-update": "bash scripts/auto-update.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:providers": "tsx scripts/test-providers.ts",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "pino": "^10.3.1",
    "ws": "^8.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.4.47",
    "prettier": "^3.8.1",
    "prisma": "^6.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.56.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "ai",
    "llm",
    "groq",
    "cerebras",
    "openai",
    "google-ai",
    "openrouter",
    "mistral",
    "huggingface",
    "failover",
    "multi-provider",
    "arbitrage",
    "free-llm",
    "ai-bus",
    "openclaw"
  ],
  "author": "Bartosz Gaca",
  "license": "MIT"
}
